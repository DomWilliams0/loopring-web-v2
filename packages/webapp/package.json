  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "publish": {
      "provider": "custom",
      "repo": "ssh://git@github.com/loopring/dexwebappv2",
      "owner": "Loopring Dex Frontend Team"
    }
  },
  "main": "./public/electron.js",
  "homepage": ".",
  "scripts": {
    "deploy_infura": "npx ipfs-deploy build > .testfile",
    "update_pinata": "sh -ac '. .env.deploy; cat .testfile | xargs -I {} npx ipfs-deploy -p pinata -d cloudflare -c {}'",
    "deploy:ipfs": "yarn deploy_infura; yarn update_pinata",
    "check_dns": "dig TXT _dnslink.alpha.loopring.pro | grep 'dnslink=/ipfs/' | awk -F '\"' {'print $2'} | awk -F '/' {'print $3'}",
    "deploy": "bash ./deploy.sh",
    "predocker": "if [ ! -d \"build\" ]; then yarn build; fi",
    "docker": "docker-compose build",
    "dev": "dotenv -e .env.development react-app-rewired start",
    "build_dev": "dotenv -e .env.development react-app-rewired build",
    "build": "dotenv -e .env.production react-app-rewired build",
    "gen_api": "cd src/lightcone_api/ && rm -rf api && java -jar openapi-generator-cli.jar generate -i openapi.yaml -g typescript-axios -o ./api"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/helper-builder-react-jsx": "^7.10.4",
